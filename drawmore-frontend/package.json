{
  "name": "drawmore-frontend",
  "version": "0.1.0",
  "description": "Frontend for DrawMore",
  "main": "index.js",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "scripts": {
    "build-dev": "npm run clean && webpack --config webpack.dev.js",
    "build-prod": "npm run clean && cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "clean": "rimraf dist",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "whiteboard"
  ],
